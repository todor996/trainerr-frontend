{
  "name": "trainerr-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development vite --host --clearScreen false",
    "build": "tsc && vite build",
    "lint": "eslint --fix . --ext ts,tsx --max-warnings 0",
    "pre-commit": "lint-staged",
    "preview": "vite preview",
    "prepare": "husky install",
    "predeploy": "pnpm run build && cd dist && cp index.html 404.html",
    "deploy": "gh-pages -d dist",
    "web": "yarn dev",
    "dev:vite:extract": "EXTRACT=1 yarn dev",
    "dev:webpack-clean": "yarn dev:webpack",
    "dev:webpack": "DISABLE_EXTRACTION=true NODE_ENV=development webpack serve",
    "dev:extract": "DISABLE_EXTRACTION=false NODE_ENV=development webpack serve",
    "prod": "NODE_ENV=production webpack --json=dist/compilation-stats.json",
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest"
  },
  "homepage": "https://futureproofsoft.github.io/trainer-frontend/",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix --ext ts,tsx,js,jsx --max-warnings 0"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@tamagui/animations-css": "^1.94.4",
    "@tamagui/colors": "^1.94.4",
    "@tamagui/core": "^1.94.4",
    "@tamagui/create-theme": "^1.94.4",
    "@tamagui/lucide-icons": "^1.94.4",
    "@tamagui/react-native-svg": "^1.94.4",
    "@types/react-fontawesome": "^1.6.8",
    "axios": "^1.6.7",
    "culori": "^4.0.1",
    "expo-linear-gradient": "^12.7.2",
    "formik": "^2.4.6",
    "i18next": "^23.7.3",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-resources-to-backend": "^1.2.0",
    "jwt-decode": "^4.0.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.3",
    "react": "^18.2.0",
    "react-daisyui": "^5.0.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-i18next": "^13.5.0",
    "react-native": "^0.74.0",
    "react-native-web": "^0.19.10",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.21.3",
    "redux-persist": "^6.0.0",
    "sort-by": "^0.0.2",
    "tailwind-merge": "^2.2.0",
    "tamagui": "^1.94.4",
    "vite-plugin-pwa": "^0.17.5",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@tamagui/vite-plugin": "^1.94.4",
    "@types/culori": "^2.0.4",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/tailwindcss": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "async-retry": "1.3.3",
    "autoprefixer": "^10.4.17",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^6.10.0",
    "daisyui": "^4.6.1",
    "esbuild-loader": "^4.1.0",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.1",
    "mini-css-extract-plugin": "^2.8.1",
