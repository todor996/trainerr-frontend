    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "tamagui-loader": "^1.94.4",
    "thread-loader": "^4.0.2",
    "typescript": "^5.3.3",
    "url-loader": "^4.1.1",
    "vite": "^4.5.2",
    "vite-plugin-checker": "^0.6.3",
    "vite-plugin-eslint": "^1.8.1",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "pnpm": {
    "overrides": {
      "lodash.pick@>=3.7.0 <4.17.19": ">=4.17.19"
    }
  }
}
